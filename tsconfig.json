{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "downlevelIteration": true,
    "outDir": "./lib",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react"
  },
  "include": ["src"],
  "exclude": ["node_modules", "lib" ,"**/__tests__/*"]
}